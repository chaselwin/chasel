package com.chasel.common.constant;

public class ResponseResult {

	private String resultStatus;

	private String resultDesc;

	private String data;

	public ResponseResult(ResponseStatus resultStatus, String resultDesc) {
		super();
		this.resultStatus = resultStatus.getCode();
		this.resultDesc = resultDesc;
	}

	public ResponseResult(ResponseStatus resultStatus, String resultDesc, String data) {
		super();
		this.resultStatus = resultStatus.getCode();
		this.resultDesc = resultDesc;
		this.data = data;
	}

	public String getResultStatus() {
		return resultStatus;
	}

	public void setResultStatus(String resultStatus) {
		this.resultStatus = resultStatus;
	}

	public String getResultDesc() {
		return resultDesc;
	}

	public void setResultDesc(String resultDesc) {
		this.resultDesc = resultDesc;
	}

	public String getData() {
		return data;
	}

	public void setData(String data) {
		this.data = data;
	}

}
